{
    "family": "sidious-fargate",
    "executionRoleArn": "arn:aws:iam::363474188884:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sidious-bot",
            "image": "sidious",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "DISCORD_TOKEN",
                    "valueFrom": "sidious_token"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
